if false then print('got hooks?') end
if PM then
   local function not_implemented(s)
      return function()
         error('Unimplemented function "'..tostring(s)..'"')
      end
   end
   local g = PM._G
   if PM.platform.hostname and PM.platform.request then
      local host = PM.platform.hostname
      local request = PM.platform.request
   end
   for _,v in pairs({
      "rconsoleprint","consoleprint",
      "rconsolename","rconsolesettitle","consolesettitle",
      "rconsoleinput","consoleinput",
      "rconsoledestroy","consoledestroy",
      "rconsolecreate","consolecreate",
      "request",
      "http_request",
      "setclipboard",
      "readfile",
      "writefile",
      "appendfile",
      "listfiles",
      "makefolder",
      "delfolder",
      "delfile",
      "isfile",
      "isfolder",
      "loadfile",
      "dofile"}) do
      if g[v]==nil then g[v]=not_implemented(v) end
   end
end